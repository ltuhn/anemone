void assignFeeling() {
  signal = analogRead(A0);
  if (forrigeFolelse == signal) {
   return; 
  }

  if (signal > 0) {
    Serial.println("Faar signal");
    if (signal == 250) {
      Serial.println("Signal: glad");
      happy_screen();
      forrigeFolelse = 250;
    }
    else if (signal == 500) {
      Serial.println("Signal: sint");
      angry_screen(); 
      forrigeFolelse = 500;
    }
    else if (signal == 750) {
      Serial.println("Signal: trist");
      sad_screen();
      forrigeFolelse = 750;
    }
    else if (signal == 1000) {
      Serial.println("Signal: neutral");
      neutral_screen(); 
      forrigeFolelse = 1000; 
    }
  }
}

